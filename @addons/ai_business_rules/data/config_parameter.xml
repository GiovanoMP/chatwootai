<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Configurações para o webhook -->
        <record id="ai_business_rules_webhook_url" model="ir.config_parameter">
            <field name="key">ai_business_rules.webhook_url</field>
            <field name="value">http://localhost:8004</field>
        </record>

        <record id="ai_business_rules_api_token" model="ir.config_parameter">
            <field name="key">ai_business_rules.api_token</field>
            <field name="value">development-api-key</field>
        </record>

        <record id="ai_business_rules_account_id" model="ir.config_parameter">
            <field name="key">ai_business_rules.account_id</field>
            <field name="value">account_1</field>
        </record>
    </data>
</odoo>
