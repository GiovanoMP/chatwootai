<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Configurações para a API -->
        <record id="business_rules_api_url" model="ir.config_parameter">
            <field name="key">business_rules.api_url</field>
            <field name="value">http://localhost:8001</field>
        </record>

        <record id="business_rules_api_token" model="ir.config_parameter">
            <field name="key">business_rules.api_token</field>
            <field name="value">default_token</field>
        </record>
    </data>
</odoo>
