<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Configurações para o webhook -->
        <record id="business_rules2_webhook_url" model="ir.config_parameter">
            <field name="key">business_rules2.webhook_url</field>
            <field name="value">http://localhost:8004</field>
        </record>

        <record id="business_rules2_api_token" model="ir.config_parameter">
            <field name="key">business_rules2.api_token</field>
            <field name="value">development-api-key</field>
        </record>
        
        <record id="business_rules2_account_id" model="ir.config_parameter">
            <field name="key">business_rules2.account_id</field>
            <field name="value">account_1</field>
        </record>
    </data>
</odoo>
