      <field name="attribute_id" ref="product_attribute_token_type" />
      <field name="value_ids" eval="[(6, 0, [
         ref('product_attribute_value_input_tokens'),
         ref('product_attribute_value_input_tokens_audio'),
         ref('product_attribute_value_input_tokens_cache_read'),
         ref('product_attribute_value_input_cached'),
         ref('product_attribute_value_output_tokens'),
         ref('product_attribute_value_output_tokens_audio'),
         ref('product_attribute_value_output_tokens_reasoning'),
         ])]" />
