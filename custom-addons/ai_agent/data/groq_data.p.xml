    </record>
    <record id="groq_product_attribute_value_llama3-70b-8192" model="product.attribute.value">
      <field name="name">llama3-70b-8192</field>
      <field name="licence">llama-community</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
    </record>
    <record id="groq_product_attribute_value_llama3-8b-8192" model="product.attribute.value">
      <field name="name">llama3-8b-8192</field>
      <field name="licence">llama-community</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
    </record>
    <record id="groq_product_attribute_value_mixtral-8x7b-32768" model="product.attribute.value">
      <field name="name">mixtral-8x7b-32768</field>
