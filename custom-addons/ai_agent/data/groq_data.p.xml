<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="groq_product_template" model="product.template">
      <field name="name">Groq</field>
      <field name="is_llm">True</field>
      <field name="llm_library">langchain_groq</field>
      <field name="llm_type">ChatGroq</field>
      <field name="fallback_api_key_name">groq_api_key</field>
      <field name="image_1920" type="base64" file="ai_agent/static/img/groq-ai-logo.png" />
    </record>
    <!-- Model -->
    <!-- https://groq.com/pricing/ -->
    <record id="groq_product_attribute_value_distil-whisper-large-v3-en" model="product.attribute.value">
      <field name="name">distil-whisper-large-v3-en</field>
      <field name="licence">mit</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
    </record>
    <record id="groq_product_attribute_value_gemma2-9b-it" model="product.attribute.value">
      <field name="name">gemma2-9b-it</field>
      <field name="licence">gemma-terms-of-use</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
    </record>
    <record id="groq_product_attribute_value_llama-3-3-70b-versatile" model="product.attribute.value">
      <field name="name">llama-3.3-70b-versatile</field>
      <field name="licence">llama-community</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
