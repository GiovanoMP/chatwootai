<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="berget_product_template" model="product.template">
      <field name="name">Berget AI</field>
      <field name="is_llm" eval="True" />
      <field name="llm_library">langchain_openai</field>
      <field name="llm_type">ChatOpenAI</field>
      <field name="fallback_api_key_name">berget_api_key</field>
      <field name="image_1920" type="base64" file="ai_agent/static/img/berget-logo.png" />
    </record>
    <!-- Model -->
    <!-- https://berget.ai/models -->
    <!-- API -->
    <!-- https://api.berget.ai/ -->
    <record id="DeepCoder_14B_Preview" model="product.attribute.value">
      <field name="name">agentica-org/DeepCoder-14B-Preview</field>
      <field name="licence">mit</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
      <field name="tpm">400000</field>
      <field name="rpm">600</field>
      <field name="has_endpoint" eval="True" />
      <field name="endpoint">https://api.berget.ai/v1/</field>
      <field name="context_window" eval="200000" />
    </record>
    <record id="Llama_3_1_8B_Instruct" model="product.attribute.value">
      <field name="name">Llama-3.1-8B-Instruct</field>
      <field name="licence">Ilama3.1</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
      <field name="tpm">400000</field>
      <field name="rpm">600</field>
      <field name="has_endpoint" eval="True" />
      <field name="endpoint">https://api.berget.ai/v1/</field>
      <field name="context_window" eval="200000" />
    </record>
    <record id="Llama_3_3_70B_Instruct" model="product.attribute.value">
      <field name="name">meta-llama/Llama-3.3-70B-Instruct</field>
      <field name="licence">Ilama3.3</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
      <field name="tpm">400000</field>
      <field name="rpm">600</field>
      <field name="has_endpoint" eval="True" />
      <field name="endpoint">https://api.berget.ai/v1/</field>
      <field name="context_window" eval="200000" />
    </record>
    <record id="unsloth_DeepSeek_R1_GGUF" model="product.attribute.value">
      <field name="name">unsloth/DeepSeek-R1-GGUF</field>
      <field name="licence">mit</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
      <field name="tpm">400000</field>
      <field name="rpm">600</field>
      <field name="has_endpoint" eval="True" />
      <field name="endpoint">https://api.berget.ai/v1/</field>
      <field name="context_window" eval="200000" />
    </record>
    <record id="Mistral_Small_3_1_24B_Instruct_2503" model="product.attribute.value">
      <field name="name">mistralai/Mistral-Small-3.1-24B-Instruct-2503</field>
      <field name="licence">apache-2.0</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
      <field name="tpm">400000</field>
      <field name="rpm">600</field>
      <field name="has_endpoint" eval="True" />
      <field name="endpoint">https://api.berget.ai/v1/</field>
      <field name="context_window" eval="200000" />
    </record>
    <!-- Attribute Line -->
    <record id="berget_ai_product_template_attribute_line_model" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="berget_product_template" />
      <field name="attribute_id" ref="product_attribute_model" />
      <field name="value_ids" eval="[(6, 0, [
                ref('DeepCoder_14B_Preview'),
                ref('Llama_3_1_8B_Instruct'),
                ref('Llama_3_3_70B_Instruct'),
                ref('unsloth_DeepSeek_R1_GGUF'),
                ref('Mistral_Small_3_1_24B_Instruct_2503'),
                    ])]" />
    </record>
    <record id="berget_product_template_attribute_line_token_type" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="berget_product_template" />
      <field name="attribute_id" ref="product_attribute_token_type" />
      <field name="value_ids" eval="[(6, 0, [
         ref('product_attribute_value_input_tokens'),
         ref('product_attribute_value_output_tokens'),
         ])]" />
    </record>
  </data>
</odoo>
