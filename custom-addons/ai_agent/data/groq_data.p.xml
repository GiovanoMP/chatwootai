      <field name="licence">llama-community</field>
      <field name="attribute_id" ref="ai_agent.product_attribute_model" />
    </record>
    <!-- Attribute Line -->
    <record id="groq_ai_product_template_attribute_line_model" model="product.template.attribute.line">
      <field name="product_tmpl_id" ref="groq_product_template" />
      <field name="attribute_id" ref="product_attribute_model" />
      <field name="value_ids" eval="[(6, 0, [
                            ref('groq_product_attribute_value_distil-whisper-large-v3-en'),
                            ref('groq_product_attribute_value_gemma2-9b-it'),
                            ref('groq_product_attribute_value_llama-3-3-70b-versatile'),
                            ref('groq_product_attribute_value_llama-3-1-8b-instant'),
                            ref('groq_product_attribute_value_llama-guard-3-8b'),
                            ref('groq_product_attribute_value_llama3-70b-8192'),
                            ref('groq_product_attribute_value_llama3-8b-8192'),
                            ref('groq_product_attribute_value_mixtral-8x7b-32768'),
                            ref('groq_product_attribute_value_whisper-large-v3'),
                            ref('groq_product_attribute_value_whisper-large-v3-turbo'),
                            ref('groq_product_attribute_value_deepseek-r1-distill-llama-70b'),
